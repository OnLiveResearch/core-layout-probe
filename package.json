    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.2.1"
  },
  "browser": {
    "angular": "./node_modules/angular/angular.js",
    "angular-iscroll": "./node_modules/angular-iscroll/dist/lib/angular-iscroll.js",
    "iscroll": "./node_modules/iscroll/build/iscroll.js",
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "ui.router": "./node_modules/angular-ui-router/release/angular-ui-router.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "angular": {
      "depends": "jquery:jQuery",
      "exports": "angular"
    },
    "angular-iscroll": {
      "depends": "angular",
      "exports": "angular.module('angular-iscroll')"
    },
    "ui.router": {
      "depends": "angular",
      "exports": "angular.module('ui.router')"
    }
  }
